<node name="/com/qcom/TypeTest" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">
	
  <interface name="org.codeaurora.mbus.echoPing">

    <method name="echo">
      <arg type="ad" name="echo_struct" direction="in" />
      <arg type="s" name="echo_string" direction="out" />
      <arg type="ai" name="echo_int" direction="out" />
      <arg type="d" name="echo_double" direction="out" />
    </method>
    
    <method name="foo">
      <arg type="ai" name="fooArg1" direction="in" />
      <arg type="ai" name="fooArg2" direction="out" />
    </method>

    <signal name="ping">
      <arg type="as" name="curr_ping_count"/>
    </signal>
    
    <signal name="bar">
      <arg type="ai" name="barArray"/>
    </signal>

  </interface>

</node>